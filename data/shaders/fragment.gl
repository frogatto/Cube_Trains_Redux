				color[2] = inputColors[colorNum][2]/255.0;
				color[3] = inputColors[colorNum][3]/255.0;
				//color = vec4(1.0, 1.0, 1.0, 1.0);
				break;
			}
		}
	}

	color *= gl_Color;
	color[1] = color[0];

	gl_FragColor = color;
}
