uniform sampler2D tex;

const vec4 OutColors[10] = vec4[](
  vec4(52.0/255.0, 28.0/255.0, 66.0/255.0, 1),
  vec4(100.0/255.0, 45.0/255.0, 85.0/255.0, 1),
